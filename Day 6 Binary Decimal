#include <iostream>
#include<math.h>
using namespace std;
int main(){
  //Decimal to binary  
  int n;
  cin>>n;
  /*
  int i=0;
  int ans=0;
  while(n!=0)
    {
      int digit =n&1;
      ans = digit* pow(10,i) +ans;
      n = n >> 1;
      i++;
    }
  cout<<ans;
*/
  //Binary TO decimal
  int i=0;
  int ans=0;
  while (n!=0)
    {
      int number =n%10;
      ans=ans+pow(2,i)*number;
      i++;
      n=n/10;
    }
  cout<<ans;
  
    
    
  
}
  
